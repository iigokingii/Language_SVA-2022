---- Исходные данные ----
numb func max(numb n, numb m)
[
 new numb result;
 state:n > m $
 correctly:[result=n;]
 wrong:[result=m;]
 $
 return result;
]; 
stroke func fs (stroke a, stroke b)
[
 new stroke c;
 new stroke func substr(stroke a, numb p, numb n);
 c = substr(a, 1,3)+ b;
 return c; 
]; 
main
[
 new numb x;
 new numb y;
 new numb z;
 new stroke sa;
 new stroke sb;
 new stroke sc;
 new numb strlen(stroke p);
 x = 1;
 y = 5;
 sa = '1234567890';
 sb = '1234567890'; 
 z = max(x,y);
 sc = fs(sa,sb);
 print 'контрольный пример';
 print z;
 print sc;
 print strlen(sc);
 return 0;
];
